    "\n",
    "    return data\n",
    "\n",
    "def save_to_csv(data, filename):\n",
    "    with open(filename, 'w', newline='') as csvfile:\n",
    "        fieldnames = ['Gender', 'Height (cm)', 'Weight (kg)', 'Hypertension', 'Low Pressure', 'Diabetes', 'Heart Condition', 'Exercise Type', 'Duration (minutes)', 'Intensity Level']\n",
